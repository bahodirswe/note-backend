{
    "builds": [
        {
            "src": "core/wsgi.py",
            "use": "@versel/python",
            "config": {
                "maxLambdasSize": "15mb",
                "runtime": "python3.9"
            }
        }
    ],
    "routes": [
        {
            "src": "/{(.*)",
            "dest": "core/wsgi.py"
        }   
    ]
}   